input_file: prqlc/prqlc/tests/integration/queries/append_group.prql
---
from invoices
select {invoice_id, customer_id, invoice_date}
append (
  from invoices
  select {invoice_id, customer_id, invoice_date}
)
group {customer_id} (aggregate {
  count = count invoice_id,
})
